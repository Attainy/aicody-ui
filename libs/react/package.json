{
  "name": "@aicody-ui/react",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build:tailwind": "tsup src/index.ts --format cjs,esm --outDir dist/tailwind",
    "build:emotion": "tsup src/index.ts --format cjs,esm --outDir dist/emotion",
    "build:vanilla": "tsup src/index.ts --format cjs,esm --outDir dist/vanilla",
    "build": "node scripts/build.cjs"
  },
  "main": "dist/tailwind/index.cjs.js",
  "module": "dist/tailwind/index.esm.js",
  "types": "./dist/index.esm.d.ts",
  "exports": {
    ".": {
      "import": "./dist/tailwind/index.esm.js",
      "require": "./dist/tailwind/index.cjs.js"
    }
  },
  "dependencies": {
    "lucide-react": "^0.487.0",
    "tailwind-variants": "^1.0.0"
  },
  "devDependencies": {
    "@emotion/css": ">=11",
    "@emotion/react": "^11.14.0",
    "@storybook/addon-styling": "^1.3.7",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@vanilla-extract/css": ">=1",
    "@vanilla-extract/esbuild-plugin": "^2.3.15",
    "rollup": "^4.14.0",
    "tailwind-merge": "^3.1.0"
  }
}
